{
  "name": "onnxruntime-github-actions",
  "version": "0.0.2",
  "description": "Reusable GitHub Actions for ONNX Runtime CI",
  "private": true,
  "scripts": {
    "build": "node esbuild.config.mjs",
    "lint": "npx eslint src/**/*.js",
    "lint:fix": "npx eslint src/**/*.js --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "ONNX Runtime"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^2.3.2",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.5.0",
    "@actions/tool-cache": "^2.0.2",
    "fast-xml-parser": "^5.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "memfs": "^4.17.0",
    "prettier": "3.5.3"
  },
  "engines": {
    "node": ">=20"
  }
}
